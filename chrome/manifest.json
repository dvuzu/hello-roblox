{
    "manifest_version": 3,
    "name": "Hello, Roblox!",
    "description": "Return the old greeting that we all missed!",
    "version": "0.2.2",
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "content_scripts": [
        {
            "js": [
                "main.js"
            ],
            "css": [
                "style.css"
            ],
            "matches": [
                "*://*.roblox.com/*home*"
            ]
        }
    ]
}